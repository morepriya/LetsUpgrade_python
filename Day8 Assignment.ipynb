   "source": [
    "You are provided with a number D containing only digits 0's and 1's. Your aim is to convert\n",
    "this number to have all the digits same."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "121\n",
      "NO\n"
     ]
    }
   ],
   "source": [
    "D=input()\n",
    "zeros = 0\n",
    "for num in D:\n",
    "    if num == '0':\n",
    "        zeros += 1\n",
    "if zeros == 1 or zeros == len(D)-1:\n",
    "    print ('YES')\n",
    "else:\n",
    "    print ('NO')\n"
   ]
  }
